# vim: set ft=zsh:

# usage: cmon <process substr>
#  cmon
#  cmon chrome
#
# print vmem usages
case $(uname -s) in
    Darwin)
        if [[ $# == 0 ]]; then
            echo 'looking around'
        else
            echo "hunting $1"
            pids=$(ps -ax | grep -i $1)
        fi
        ;;
    *)
        echo 'todo'
        ;;
esac
    
